<Application x:Class="Market.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:adonisUi="clr-namespace:AdonisUI;assembly=AdonisUI"
             xmlns:converters="clr-namespace:Market.Converters"
             xmlns:local="clr-namespace:Market">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/AdonisUI;component/ColorSchemes/Light.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/AdonisUI.ClassicTheme;component/Resources.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <converters:NullableConverter x:Key="NullableConverter" />
            <converters:NonNullableConverter x:Key="NoneNullableConverter" />
            <converters:BoolenToVisibleConverter x:Key="BooleanToVisibleConverter" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
